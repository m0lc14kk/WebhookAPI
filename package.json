{
    "name": "webhook-api",
    "description": "A typing library to easily create content of Discord webhook messages.",
    "version": "1.0.0",
    "scripts": {
        "build": "tsup && node --no-deprecation --experimental-strip-types \"./scripts/build.mts\"",
        "pretty": "pnpx prettier --write \"./**/*.{ts,js,mjs,json,yaml}\"",
        "lint": "pnpm eslint .",
        "prepare": "husky"
    },
    "keywords": [
        "webhook-api",
        "discord webhooks",
        "minecraft discord"
    ],
    "author": "m0lc14kk",
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.11.0",
    "dependencies": {
        "@minecraft/server-net": "1.0.0-beta.1.21.81-stable"
    },
    "devDependencies": {
        "@eslint/js": "^9.26.0",
        "@eslint/json": "^0.12.0",
        "@minecraft/server": "2.1.0-beta.1.21.93-stable",
        "eslint": "^9.26.0",
        "globals": "^16.1.0",
        "husky": "^9.1.7",
        "prettier": "^3.5.3",
        "tsup": "^8.5.0",
        "typescript-eslint": "^8.32.1"
    }
}
